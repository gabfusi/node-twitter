{
  "name": "twitter-enhanced",
  "version": "1.4.3",
  "description": "Twitter API client library for node.js, enhanced with Stream API POST requests",
  "keywords": [
    "twitter",
    "streaming",
    "oauth"
  ],
  "homepage": "https://github.com/gabfusi/node-twitter",
  "author": "Gabriele Fusi",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabfusi/node-twitter"
  },
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "jshint test/*.js lib/*.js && echo Lint passed."
  },
  "dependencies": {
    "deep-extend": "^0.3.2",
    "request": "^2.51.0"
  },
  "devDependencies": {
    "jshint": "^2.6.0",
    "mocha": "^2.1.0",
    "nock": "^0.57.0"
  },
  "main": "./lib/twitter",
  "jshintConfig": {
    "boss": true,
    "node": true,
    "strict": true,
    "newcap": false,
    "undef": true,
    "unused": true,
    "onecase": true,
    "lastsemic": true,
    "indent": 2,
    "globals": {
      "escape": true,
      "describe": true,
      "it": true,
      "before": true
    }
  }
}
